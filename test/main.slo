uporabi model.converter;

odkrit razred Program {

	globalno odkrito celo fakulteta(celo vhod) {
		celo rezultat = 1;
		za (celo i = 1; i <= vhod; i++) {
			rezultat = rezultat * i;
		}
		vrni rezultat;
	}
	
	globalna odkrita praznina zagon() {
		Konzola.printaj("vnesi stevilo");
		niz vnos = Konzola.preberi();
		celo vrednost = Converter.nizVcelo(vnos);
		Konzola.printaj("Reultat je:");
		celo rezultat = fakulteta(vrednost);
		Konzola.printaj(rezultat);
	}

}
