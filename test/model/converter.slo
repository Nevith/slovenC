odkrit razred Converter {
	
	globalno odkrito celo dajStevko(niz stevka) {
		ko (stevka == "1") {
			vrni 1;
		}
		ko (stevka == "2") {
			vrni 2;
		}
		ko (stevka == "3") {
			vrni 3;
		}
		ko (stevka == "4") {
			vrni 4;
		}
		ko (stevka == "5") {
			vrni 5;
		}
		ko (stevka == "6") {
			vrni 6;
		}
		ko (stevka == "7") {
			vrni 7;
		}
		ko (stevka == "8") {
			vrni 8;
		}
		ko (stevka == "9") {
			vrni 9;
		}
		ko (stevka == "0") {
			vrni 0;
		}
		
		Konzola.printaj("Napacen vnos");
		vrni 0;
	}
	
	globalno odkrito celo potenca(celo osnova, celo moc) {
		ko (moc == 0) {
			vrni 1;
		}
		za (celo i = 1; i <= moc; i++) {
			osnova*osnova;
		}
		vrni osnova;
	}
	
	globalno odkrito celo nizVcelo(niz stevilo) {
		celo dolzina = stevilo.dolzina();
		celo rezultat = 0;
		za (celo i = 0; i < dolzina; i++) {
			niz stevka = stevilo.vzemi(i);
			celo vrednost = dajStevko(stevka);
			rezultat = rezultat + (vrednost * potenca(10, dolzina-i-1));
		}
		vrni rezultat;
	}
}